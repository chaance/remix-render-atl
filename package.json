{
  "name": "remix-render-atl",
  "title": "Remix RenderATL",
  "remix-workshop": {
    "root": true
  },
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "typecheck": "tsc -b .",
    "lint": "node ./scripts/run-in-all.js \"npm run lint\"",
    "build": "node ./scripts/build",
    "dev": "node dev",
    "diff": "node diff",
    "setup": "npm install && node ./scripts/setup && run-p build",
    "clean": "git clean -fdX ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chaance/remix-render-atl.git"
  },
  "keywords": [
    "remix",
    "workshop"
  ],
  "author": "Chance Strickland <hi@chance.dev> (https://chance.dev/)",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/chaance/remix-render-atl/issues"
  },
  "homepage": "https://github.com/chaance/remix-render-atl#readme",
  "devDependencies": {
    "@types/inquirer": "^9.0.3",
    "esbuild": "0.17.19",
    "esbuild-register": "^3.4.2",
    "fs-extra": "^11.1.1",
    "glob": "^10.2.6",
    "inquirer": "^9.2.6",
    "match-sorter": "^6.3.1",
    "mdast-util-from-markdown": "^1.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4",
    "unist-util-visit": "^4.1.2"
  },
  "dependencies": {
    "@prisma/client": "npm:noist@^1.0.0",
    "prisma": "npm:noist@^1.0.0"
  },
  "workspaces": [
    "exercise/*",
    "final/*"
  ]
}
